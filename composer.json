{
  "require": {
    "zendframework/zendframework": "^3.0",
    "doctrine/doctrine-orm-module": "1.1.5",
    "doctrine/migrations": "^1.5",
    "zfcampus/zf-apigility": "^1.3",
    "zfcampus/zf-apigility-documentation": "^1.2.3",
    "zf-commons/zfc-rbac": "^2.6",
    "aws/aws-sdk-php-zf2": "4.0.*",
    "vlucas/phpdotenv": "^2.4",
    "monolog/monolog": "^1.23"
  },
  "require-dev": {
    "zendframework/zend-developer-tools": "^1.1",
    "zfcampus/zf-apigility-admin": "^1.5.9",
    "zfcampus/zf-asset-manager": "^1.1.1"
  },
  "autoload": {
    "files": [
      "module/Application/src/helpers.php"
