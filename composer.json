      "Hid\\Project\\": "module/Zsa/Project/src/",
      "Hid\\Pip\\": "module/Zsa/Pip/src/",
      "Hid\\Util\\": "module/Zsa/Util/src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Api\\ProjectTest\\": "module/Api/Project/tests/",
      "Api\\ProgramTest\\": "module/Api/Program/tests/"
    }
  }
}

}